{
  "expo": {
    "name": "Medicare",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_MediCare.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "package": "com.medicare.app",
      "bundleIdentifier": "com.medicare.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.medicare.app",
      "adaptiveIcon": {
        "backgroundColor": "#2ccdd2",
        "foregroundImage": "./assets/images/android-icon-foreground.jpg",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.jpeg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo_MediCare.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_MediCare.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "47e21c9f-17af-41a7-a87d-3d0177642eee"
      }
    },
    "owner": "amadouseye001"
  }
}
