{
  "expo": {
    "name": "Medicare",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoMedicare.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "package": "com.medicare.app",
      "bundleIdentifier": "com.medicare.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "MediCare a besoin d'accéder à votre localisation pour envoyer une alerte d'urgence à votre médecin.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "MediCare a besoin d'accéder à votre localisation pour envoyer une alerte d'urgence à votre médecin."
      }
    },
    "android": {
      "package": "com.medicare.app",
      "adaptiveIcon": {
        "backgroundColor": "#2ccdd2"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logoMedicare.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logoMedicare.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "47e21c9f-17af-41a7-a87d-3d0177642eee"
      }
    },
    "owner": "amadouseye001"
  }
}
